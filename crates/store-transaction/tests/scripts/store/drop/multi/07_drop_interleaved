# Tests interleaved insert/drop/insert operations.

# Initial insert
set a=1 version=1
---
ok

get a version=1
---
"a" => "1"

# Drop it
drop a
---
ok

get a version=1
---
"a" => None

# Re-insert at new version
set a=new version=2
---
ok

get a version=2
---
"a" => "new"

# Drop and re-insert again in same batch
drop a
set a=final version=3
---
ok

get a version=3
---
"a" => "final"
