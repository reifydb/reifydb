# Tests keep_last_versions=0 - same effect as drop all.

set a=1 version=1
set a=2 version=2
set a=3 version=3
---
ok

count_versions a
---
"a" => 3 versions

# Keep 0 means drop all
drop a keep_last_versions=0
---
ok

# All versions gone
count_versions a
---
"a" => 0 versions

contains a version=1
contains a version=2
contains a version=3
---
"a" => false
"a" => false
"a" => false
