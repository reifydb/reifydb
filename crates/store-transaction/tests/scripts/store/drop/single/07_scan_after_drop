# Tests that scan doesn't show dropped keys.

set a=1
set b=2
set c=3
---
ok

scan
---
"a" => "1"
"b" => "2"
"c" => "3"

# Verify 'b' exists
contains b
---
"b" => true

drop b
---
ok

# 'b' no longer exists
contains b
---
"b" => false

# Scan should not show 'b'
scan
---
"a" => "1"
"c" => "3"
