# Tests basic drop in single-version storage.

set a=1
set b=2
---
ok

# Verify both keys exist
contains a
contains b
---
"a" => true
"b" => true

get a
get b
---
"a" => "1"
"b" => "2"

# Drop 'a'
drop a
---
ok

# 'a' no longer exists, 'b' still exists
contains a
contains b
---
"a" => false
"b" => true

get a
get b
---
"a" => None
"b" => "2"
