# Tests drop after updating a key.

set a=initial
---
ok

contains a
---
"a" => true

get a
---
"a" => "initial"

set a=updated
---
ok

contains a
---
"a" => true

get a
---
"a" => "updated"

drop a
---
ok

contains a
---
"a" => false

get a
---
"a" => None
