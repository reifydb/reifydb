# Tests interleaved set/drop operations.

set a=1
---
ok

contains a
---
"a" => true

get a
---
"a" => "1"

drop a
---
ok

contains a
---
"a" => false

get a
---
"a" => None

# Re-insert after drop
set a=new
---
ok

contains a
---
"a" => true

get a
---
"a" => "new"
