# Tests dropping on a key that doesn't exist.
# This should be a no-op.

# Initial empty state
stats
---
current_count: 0
current_key_bytes: 0
current_value_bytes: 0
historical_count: 0
historical_key_bytes: 0
historical_value_bytes: 0
cdc_count: 0
cdc_key_bytes: 0
cdc_value_bytes: 0
total_bytes: 0

# Drop nonexistent key
drop missing version=1
---
ok

# Stats should remain at 0
stats
---
current_count: 0
current_key_bytes: 0
current_value_bytes: 0
historical_count: 0
historical_key_bytes: 0
historical_value_bytes: 0
cdc_count: 0
cdc_key_bytes: 0
cdc_value_bytes: 0
total_bytes: 0
