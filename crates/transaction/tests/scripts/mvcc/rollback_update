t1: begin
t1: set a=1
t1: commit
---
ok

t2: begin
t2: set a=2
t2: set b=1
t2: set a=3
t2: rollback
---
ok

t3: begin
t3: scan
---
t3: "a" → "1"

# Everything is rolled back for a new TX
t4: begin
t4: scan
---
t4: "a" → "1"

# Everything is rolled back for a new RX
t5: begin readonly
t5: scan
---
t5: "a" → "1"
