# Test UTF-8 characters in string literals
tokenize 'from [
    { id: 1, name: "Zebra"   },
    { id: 2, name: "Apple"   },
    { id: 3, name: "中文"    },
    { id: 4, name: "العربية" },
    { id: 5, name: "Banana"  }
  ] insert test.unicode_names'
