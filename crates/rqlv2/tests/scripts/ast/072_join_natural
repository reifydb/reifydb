# Test simple NATURAL JOIN
parse 'NATURAL JOIN { FROM orders } AS o'
---
> Program
> `-- [0] Expression
>     `-- Join(Natural)
>         +-- subquery:
>         |   `-- SubQuery
>         |       `-- [0] From(default.orders)
>         `-- alias: o
>

# Test NATURAL JOIN with longer alias
parse 'NATURAL JOIN { FROM orders } AS ord'
---
> Program
> `-- [0] Expression
>     `-- Join(Natural)
>         +-- subquery:
>         |   `-- SubQuery
>         |       `-- [0] From(default.orders)
>         `-- alias: ord
>

# Test NATURAL LEFT JOIN
parse 'NATURAL LEFT JOIN { FROM orders } AS o'
---
