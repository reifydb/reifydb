# Test simple FILTER expression
parse 'FROM users | FILTER active'
---
> Program
> `-- [0] Pipeline
>     +-- [0] From(default.users)
>     `-- [1] Filter
>         `-- Identifier(active)
>

# Test FILTER with single identifier predicate
parse 'FROM products | FILTER visible'
---
> Program
> `-- [0] Pipeline
>     +-- [0] From(default.products)
>     `-- [1] Filter
>         `-- Identifier(visible)
>

# Test FILTER with boolean literal
parse 'FROM users | FILTER true'
---
> Program
> `-- [0] Pipeline
>     +-- [0] From(default.users)
>     `-- [1] Filter
>         `-- Bool(true)
>
