# Set up an initial dataset of keys with overlapping or adjacent prefixes.
set a=1
set b=2
set ba=21
set bb=22
set "b\xff"=2f
set "b\xff\x00"=2f0
set "b\xffb"=2fb
set "b\xff\xff"=2ff
set c=3
set "\xff"=f
set "\xff\xff"=ff
set "\xff\xff\xff"=fff
set "\xff\xff\xff\xff"=ffff
scan
---
"a" → "1"
"b" → "2"
"ba" → "21"
"bb" → "22"
"b\xff" → "2f"
"b\xff\x00" → "2f0"
"b\xffb" → "2fb"
"b\xff\xff" → "2ff"
"c" → "3"
"\xff" → "f"
"\xff\xff" → "ff"
"\xff\xff\xff" → "fff"
"\xff\xff\xff\xff" → "ffff"
