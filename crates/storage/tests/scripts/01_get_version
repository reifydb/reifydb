set a=1 version=1
set a=2 version=2
set a=3 version=3
---
ok

get a version=3
---
"a" => "3"

get a version=2
---
"a" => "2"

get a version=1
---
"a" => "1"

get a version=0
---
"a" => None

# version is lower bound - if not overwritten in the future it will be found

get a version=4
---
"a" => "3"

get a version=1024
---
"a" => "3"
