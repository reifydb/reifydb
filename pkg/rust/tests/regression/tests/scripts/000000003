(admin 'create namespace test')
(admin 'create table test.arith{id: int2, value: int2, num: int2}')
(command '
  INSERT test.arith [
    { id: 1, value: 1, num: 5  },
    { id: 1, value: 1, num: 10 },
    { id: 1, value: 2, num: 15 },
    { id: 2, value: 1, num: 10 },
    { id: 2, value: 1, num: 30 }
  ]
')
command 'from test.arith
    map { 1 + 2, 3 + 4, X: 4 + 5 }
'
---
> +----------+-------+-------+-----+
> |  rownum  |  1+2  |  3+4  |  X  |
> +----------+-------+-------+-----+
> |    5     |   3   |   7   |  9  |
> |    4     |   3   |   7   |  9  |
> |    3     |   3   |   7   |  9  |
> |    2     |   3   |   7   |  9  |
> |    1     |   3   |   7   |  9  |
> +----------+-------+-------+-----+
>
