(command 'create namespace test')
(command 'create table test.arith{id: int2, value: int2, num: int2}')
(command '
  from [
    { id: 1, value: 1, num: 5  },
    { id: 1, value: 1, num: 10 },
    { id: 1, value: 2, num: 15 },
    { id: 2, value: 1, num: 10 },
    { id: 2, value: 1, num: 30 }
  ] insert test.arith
')
command '
    from test.arith
    map 1 as a
'
---
> +-------------------+-----+
> |  __ROW__NUMBER__  |  a  |
> +-------------------+-----+
> |         5         |  1  |
> |         4         |  1  |
> |         3         |  1  |
> |         2         |  1  |
> |         1         |  1  |
> +-------------------+-----+
> 
