(admin 'create namespace test')

(admin 'create table test.conversion{col: uint4}')
(command 'INSERT test.conversion FROM [{ col: 0 }]')
command 'from test.conversion map cast(col, float8)'
---
> +----------+-------+
> |  rownum  |  col  |
> +----------+-------+
> |    1     |   0   |
> +----------+-------+
>


(command 'INSERT test.conversion FROM [{ col: 4294967295 }]')
command 'from test.conversion map cast(col, float8)'
---
> +----------+--------------+
> |  rownum  |     col      |
> +----------+--------------+
> |    2     |  4294967295  |
> |    1     |      0       |
> +----------+--------------+
>
