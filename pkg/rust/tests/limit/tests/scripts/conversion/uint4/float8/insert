(command 'create namespace test')

(command 'create table test.conversion{col: float8}')

(command 'from [{ col: cast(cast(0, uint4), float8) }] insert test.conversion')
command 'from test.conversion map col'
---
> +-------------------+-------+
> |  __ROW__NUMBER__  |  col  |
> +-------------------+-------+
> |         1         |   0   |
> +-------------------+-------+
> 

(command 'from [{ col: cast(cast(4294967295, uint4), float8) }] insert test.conversion')
command 'from test.conversion map col'
---
> +-------------------+--------------+
> |  __ROW__NUMBER__  |     col      |
> +-------------------+--------------+
> |         2         |  4294967295  |
> |         1         |      0       |
> +-------------------+--------------+
> 
