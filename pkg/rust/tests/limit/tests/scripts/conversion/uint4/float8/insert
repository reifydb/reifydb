(admin 'create namespace test')

(admin 'create table test.conversion{col: float8}')

(command 'INSERT test.conversion FROM [{ col: cast(cast(0, uint4), float8) }]')
command 'from test.conversion map col'
---
> +----------+-------+
> |  rownum  |  col  |
> +----------+-------+
> |    1     |   0   |
> +----------+-------+
>

(command 'INSERT test.conversion FROM [{ col: cast(cast(4294967295, uint4), float8) }]')
command 'from test.conversion map col'
---
> +----------+--------------+
> |  rownum  |     col      |
> +----------+--------------+
> |    2     |  4294967295  |
> |    1     |      0       |
> +----------+--------------+
>
