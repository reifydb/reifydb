{
  "name": "@reifydb/reifydb-ts",
  "type": "module",
  "version": "0.0.1",
  "description": "A TypeScript gRPC client for ReifyDB",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "generate": "protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts_proto --ts_out=./src/generated --proto_path=./proto ./proto/reifydb.proto",
    "test": "node --import 'data:text/javascript,import { register } from \"node:module\"; import { pathToFileURL } from \"node:url\"; register(\"ts-node/esm\", pathToFileURL(\"./\"));' src/client.ts",
    "build": "tsc --project tsconfig.json"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.15",
    "@grpc/proto-loader": "^0.7.6",
    "long": "^5.3.2",
    "protobufjs": "^7.5.3",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "ts-proto": "^1.164.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "reifydb"
  ],
  "files": [
    "dist/",
    "package.json",
    "readme.md",
    "license.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}