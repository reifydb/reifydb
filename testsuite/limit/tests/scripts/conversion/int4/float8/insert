(command 'create namespace test')

(command 'create table test.conversion{col: float8}')

(command 'from [{ col: cast(cast(0, int4), float8) }] insert test.conversion')
command 'from test.conversion map col'
---
> +-------+
> |  col  |
> +-------+
> |   0   |
> +-------+
> 

(command 'from [{ col: cast(cast(-2147483648, int4), float8) }] insert test.conversion')
command 'from test.conversion map col'
---
> +---------------+
> |      col      |
> +---------------+
> |  -2147483648  |
> |       0       |
> +---------------+
> 

(command 'from [{ col: cast(cast(2147483647, int4), float8) }] insert test.conversion')
command 'from test.conversion map col'
---
> +---------------+
> |      col      |
> +---------------+
> |  2147483647   |
> |  -2147483648  |
> |       0       |
> +---------------+
> 
