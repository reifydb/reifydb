(tx 'create schema test')
(tx 'create table test.arith(id: int2, value: int2, num: int2)')
(tx 'insert (1,1,5), (1,1,10), (1,2,15), (2,1,10), (2,1,30) into test.arith(id,value,num)')
tx 'from test.arith
    map 1 + 2, 3 + 4, 4 + 5 as X
'
---
> +-------+-------+-----+
> |  1+2  |  3+4  |  X  |
> +-------+-------+-----+
> |   3   |   7   |  9  |
> |   3   |   7   |  9  |
> |   3   |   7   |  9  |
> |   3   |   7   |  9  |
> |   3   |   7   |  9  |
> +-------+-------+-----+
> 
